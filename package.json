{
    "name": "webpacktemplate",
    "version": "1.0.0",
    "description": "My template",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --mode development",
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
        "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
        "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
        "stylelint": "stylelint-config-prettier-check \"src/styles/**/*.scss\" --syntax=scss --fix",
        "eslint": "eslint \"src/*/**/*.{js,ts,tsx}\" --quiet --fix --ignore-path .gitignore .",
        "format": "prettier --write \"src/*/**/*.+(js|jsx|json|css|scss|md)\""
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/polyfill": "^7.8.7",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-typescript": "^7.10.4",
        "@tsconfig/recommended": "^1.0.1",
        "@types/node": "^14.11.8",
        "@typescript-eslint/eslint-plugin": "^4.1.0",
        "@typescript-eslint/parser": "^4.1.0",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "bemto.pug": "^2.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.0.2",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.2",
        "css-loader": "^4.3.0",
        "css-mqpacker": "^7.0.0",
        "cssnano": "^4.1.10",
        "csv-loader": "^3.0.2",
        "cz-customizable": "^6.2.0",
        "dotenv": "^8.2.0",
        "eslint": "7.8.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "favicons-webpack-plugin": "^4.2.0",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "iconfont-plugin-webpack": "^1.1.4",
        "import-glob-loader": "^1.1.0",
        "mini-css-extract-plugin": "^0.11.1",
        "node-sass": "^4.13.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "postcss-pxtorem": "^5.1.1",
        "prettier": "^2.0.5",
        "prettier-webpack-plugin": "^1.2.0",
        "pug": "^3.0.0",
        "pug-plain-loader": "^1.0.0",
        "raw-loader": "^4.0.1",
        "sass-loader": "^10.0.2",
        "sort-css-media-queries": "^1.5.0",
        "stylelint": "^13.6.0",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-prettier": "^1.1.2",
        "stylelint-webpack-plugin": "^2.0.0",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^4.1.0",
        "ts-loader": "^8.0.3",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.0.2",
        "webpack": "^4.41.5",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "normalize.css": "^8.0.1"
    }
}
